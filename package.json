{
  "name": "hdo-front",
  "description": "Frontend for holderdeord.no.",
  "version": "0.0.1-16",
  "author": "Holder de ord contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/holderdeord/hdo-front"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "setup": "npm install && ./node_modules/.bin/bower install",
    "postdeploy": "bower install",
    "start": "node server.js",
    "start-dev": "./node_modules/.bin/nodemon --ignore ./node_modules/ server.js",
    "test": "./node_modules/.bin/jshint server.js lib/ && ./node_modules/.bin/mocha -R spec test{,/**}",
    "tdd": "./node_modules/.bin/mocha --watch -R spec test{,/**}"
  },
  "dependencies": {
    "express": "^4.0.0",
    "express-hbs": "^0.7.9",
    "browserify-middleware": "^2.3.0",
    "lodash": "^2.4.1",
    "bluebird": "^1.2.4",
    "node-sass": "^0.8.6",
    "jquery": "^2.1.0",
    "morgan": "~1.0.0",
    "uri-template": "~0.4.1",
    "moment": "~2.6.0",
    "halfred": "^0.2.0"
  },
  "devDependencies": {
    "jshint": "^2.4.4",
    "bower": "^1.3.1",
    "nodemon": "^1.0.19",
    "chai": "^1.9.0",
    "mocha": "^1.18.0",
    "nock": "^0.32.2"
  },
  "subdomain": "hdo-front",
  "domains": [
    "front.holderdeord.no"
  ]
}
