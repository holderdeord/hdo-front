        <hr>

        <div class="row">
            <div class="col-md-6">
                <img src="{{logo}}">
            </div>

            <div class="col-md-6 text-right">
                <ul class="list-unstyled">
                {{#representatives}}
                    <li>
                        <a href="/representanter/{{slug}}">
